{
  "name": "join-test-assigment",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/yarikgenza/join-test-assigment.git",
  "author": "Yarik Henza <gsmeter@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && yarn run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --coverage",
    "pretest": "cross-env NODE_ENV=test yarn run migrate:reset",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "cross-env": "^7.0.2",
    "jest": "^26.4.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
